{
  "name": "brainstorm",
  "version": "1.0.0",
  "engines": {
    "node": "10.16.0"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/src/index.js",
    "dev": "babel-watch -L ./src/index.js",
    "build": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules",
    "lint": "eslint --fix 'src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.{js|json}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Gabriel Urena <gabriel.uh@hotmail.com>",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nodemonConfig": {
    "ext": "js",
    "ignore": [
      ".git",
      "./node_modules/*"
    ]
  },
  "dependencies": {
    "@hapi/joi": "17.1.0",
    "amqplib": "0.5.5",
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "joi-objectid": "3.0.1",
    "mongoose": "5.8.11"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "babel-watch": "7.0.0",
    "eslint": "6.8.0",
    "husky": "4.2.1",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1"
  }
}
