{
  "name": "quiz-up",
  "version": "1.0.0",
  "engines": {
    "node": "10.16.0"
  },
  "description": "Manager for panel based question games",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/src/index.js",
    "dev": "babel-watch -L ./src/index.js",
    "build": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules",
    "lint": "eslint --fix 'src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.{js|json}'"
  },
  "keywords": [],
  "author": "ECT PUCMM",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.0",
    "@hapi/joi": "17.1.0",
    "amqplib": "0.5.5",
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "joi-objectid": "3.0.1",
    "mongoose": "5.8.11"
  },
  "devDependencies": {
    "babel-watch": "7.0.0",
    "eslint": "6.8.0",
    "husky": "4.2.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1"
  }
}
